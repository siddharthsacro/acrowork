    <script>

        function runScript(e) {
            AlphaNumericSpecialwithemail(e);

            if (e.keyCode == 13) {
                $("[id*=btnexisting]").click(function () { return false })
                $('#searchtextbox').val($('#searchtextbox').val().replace('’', "'"));
                SearchRecords();
                return false;
            }
        }
        function SearchRecords() {
            $('#searchtextbox').val($('#searchtextbox').val().replace('’', "'"));
            if ($('#searchtextbox').val().trim().length < 3) {
                alrtMessageList.length = 0;
                //alert("Please enter minimum 3 letters of Client information to search !");
                alrtMessageList.push({ msg: alertMessages.msgMinThreeChar, id: "searchtextbox", cls: "" });
                return alertSummary(alrtTitle, alrtMessageList);
                // alertCommonPopup(alrtSuccessTitle, alertMessages.msgSaveAppointmnt);
                //  $('#searchtextbox').focus();
                //  return false;
            }
            if ($('[id*=hdfpagename]').val() == '1') // 1 means confirm dialogue box will apper
            {
                //  var r = false;//confirm("You are being redirected to another page, Do you want to continue?");
                $("#hdnRedirect").val("searchRedirectPage");
                return confirmCommonPopup(alrtConfirmation, alertMessages.msgRedirectToAnotherPage);

            }

           <%-- if ($('#searchtextbox').val().trim().length < 3) {
                alert("Please enter minimum 3 letters of Client information to search !");
                $('#searchtextbox').focus();
                return false;
            }--%>
            else {

                __doPostBack('<%= btnsearch.UniqueID%>', '');

            }
            return false;
        }

        function AlphaNumericSpecialwithemail(event) {
            var illigalChar = /[^a-zA-Z0-9-@', ./]/g;

            if (illigalChar.test(String.fromCharCode(event.which))) {
                event.preventDefault();
                return false;
            }
        }

    </script>


