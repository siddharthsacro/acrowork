    <script type="text/javascript">

        <%--var sessionTimeout = "<%= Session.Timeout %>";
        function GetSessionTimeOut() {
            $('[id*=LblSessionTime]').empty();
            sessionTimeout = sessionTimeout - 1;
            $('[id*=LblSessionTime]').append(sessionTimeout);
            //if session is not less than 0
            if (sessionTimeout == 5) {

                alrtMessageList.length = 0;
                alrtMessageList.push({ msg: alertMessages.SessionOver + ' ' + sessionTimeout + ' Minutes', id: '', cls: "" });
                return alertSummary(alrtTitle, alrtMessageList);
            }
            else if (sessionTimeout == 0) {
                window.open("../../Login.aspx");
            }

            //return alertSummary(alrtTitle, alrtMessageList);

        }--%>
    </script>

